{
  "name": "@ttab/typescript-api-client",
  "version": "0.0.1",
  "description": "Browser-friendly TypeScript client for TT Nyhetsbyrån public APIs.",
  "keywords": [
    "api",
    "client",
    "browser",
    "news",
    "content",
    "tt",
    "tt nyhetsbyrån"
  ],
  "scripts": {
    "generate": "ts-node generator/generate.ts -p tsconfig.json > api.ts && prettier --write api.ts",
    "dist": "npm run generate && tsc -p tsconfig-dist.json",
    "prepublishOnly": "npm run dist",
    "test": "jest"
  },
  "main": "./dist/api.js",
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.1",
    "@types/jest": "^24.0.5",
    "@types/lodash": "^4.14.120",
    "@types/mustache": "^0.8.32",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "mustache": "^3.0.1",
    "prettier": "^1.16.4",
    "swagger-typescript-codegen": "^1.9.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  }
}
